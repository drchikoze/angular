RewriteEngine on

RewriteRule ^/js/a - [L]

RewriteCond %{REQUEST_URI} ^/icache/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule .* icache.php?uri=%{REQUEST_URI} [L]

php_value display_errors on
php_value error_reporting 2047
php_value xdebug.remote_enable 1
php_value xdebug.remote_host 192.168.164.206

# ƒл€ админки надо раскомментировть строки ниже
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^admin admin.php [L]

# ƒве политики файлов:
# 1. на index.php перенаправл€ютс€ все вызовы, которые не файлы
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule .* index.php

# 2. на index.php перенаправл€ютс€ все вызовы по маске (в т.ч. если файл существует
#RewriteRule !\.(js|ico|gif|jpg|png|css)$ index.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule (.*)-version-(\d+)\.(jpg|jpeg|gif|png|js|css|txt|htm)$ $1.$3 [R,L]

